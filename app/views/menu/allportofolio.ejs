    <script src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js"></script>

    <script>
      $(document).ready(function () {
        $('#select')
          .on('change', function () {
            const value = $(this).val();
            // alert(value);
            if (value == 'all') {
              $('.card-portfolio').show('1000');
            } else {
              $('.card-portfolio')
                .not('.' + value)
                .hide(1000);
              $('.card-portfolio')
                .filter('.' + value)
                .show(1000);
            }
          })
          .change();
      });
    </script>

    <div class="card-section">
      <div class="head-card">
        <h1>OTHER PROJECTS</h1>
        <select name="select" id="select" style="margin-left: 1rem; background-color: black; color: white; border: 2px solid #f7b716; border-radius: 5px">
          <option value="all">All</option>
          <option value="villa">Villa</option>
          <option value="rumah">Rumah</option>
        </select>
      </div>
      <br />
      <div class="container-cards">
        <% if (AllProject.length !== 0 ) { %> <% AllProject.forEach(p => { %>
        <a href="/portofolio/<%= p.id_project %>" class="card-portfolio <%= p.select_project %>" id="<%= p.select_project %>">
          <img src="../../<%= p.gambar_project[0].path %> " />
          <div class="card-bodys">
            <h5 class="title-card"><%= p.judul_project %></h5>
            <hr />
            <span class="badge" style="background-color: blue; border-radius: 5px; padding: 0 1rem 0 1rem"><%= p.select_project %></span>
            <p class="text-card"><%= p.desc_project %></p>
          </div>
        </a>
        <% }) %> <% } else { %>
        <h1 style="text-align: center; color: white">404 Tidak ada Data</h1>
        <% } %>
      </div>
